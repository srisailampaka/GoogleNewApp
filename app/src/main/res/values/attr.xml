<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomFontTextView">
        <attr name="fontName"/>
    </declare-styleable>

    <attr name="fontName" format="enum">
        <enum name="montserrat_bold" value="1" />
        <enum name="montserrat_light" value="2" />
        <enum name="montserrat_medium" value="3" />
        <enum name="montserrat_regular" value="4" />
    </attr>
</resources>
